{
    "private": true,
    "name": "test-nightwatch-chrome-headless",
    "description": "Test project to setup Nightwatch.js (Selenium) + Chrome Headless",
    "version": "0.1.1",
    "license": "MIT",
    "author": {
        "name": "Piotr Kowalski",
        "email": "piecioshka@gmail.com",
        "url": "https://piecioshka.pl/"
    },
    "scripts": {
        "clear": "rm -rf reports/ screenshots/",
        "clear:all": "rm -rf node_modules/ && npm run clear",
        "test": "npm run test:e2e",
        "test:e2e": "nightwatch --config nightwatch.conf.js",
        "selenium:start": "selenium-standalone install && selenium-standalone start"
    },
    "devDependencies": {
        "nightwatch": "^1.0.8",
        "selenium-standalone": "^6.15.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/piecioshka/test-nightwatch-chrome-headless.git"
    }
}
